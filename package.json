{
  "name": "tetris",
  "version": "1.0.0",
  "author": "Milan K.",
  "description": "Tetris game made with Typescript, React.js, Electron.io",
  "scripts": {
    "clean": "gulp clean",
    "watch": "gulp dev",
    "start": "electron dist/index.js",
    "build": "npm i & gulp build & electron-builder"
  },
  "main": "dist/index.js",
  "build": {
    "artifactName": "Tetris.${ext}",
    "directories": {
      "buildResources": "dist",
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "target": "portable",
      "icon": "dist/icon.ico"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "electron": "^8.5.2",
    "electron-builder": "^22.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-stylelint": "^13.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "stylelint": "^13.2.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.14.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.0"
  },
  "dependencies": {
    "normalize-scss": "^7.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "tslib": "^1.11.1",
    "uuid": "^7.0.2"
  }
}
